<resources>
    <string name="app_name" translatable="false">Resources-1</string>
    <string name="txt_hello_world">Hello World!</string>
    <string name="txt_hello_android">Hello, Android</string>

    <!--
      - %d - placeholder.
      - % - placeholder start,
      - d - type of the argument
    -->
    <string name="cats_in_house" translatable="false">Cats in the house: %d</string>

    <!-- 1$s - defines the number of the passed argument and its type -->
    <string name="animal_name" translatable="false">Name of the %1$s is %2$s. %2$s is a %1$s</string>

    <!-- .2f - number of digits after decimal separator -->
    <string name="cat_weight" translatable="false">The cat weights %.2f kilos</string>

    <!-- t - argument type - time, b,e,Y - flags specific for this type -->
    <string name="cats_birthday" translatable="false">Cat\'s birthday is on: %1$tb %1$te %1$tY</string>

    <string name="hint_cat_name">Cat name</string>

    <!-- String array -->
    <array name="cat_names">
        <item>Murzik</item>
        <item>Murka</item>
        <item>Pushok</item>
    </array>

    <!-- Plurals, see other languages for details -->
    <plurals name="cats_quantity">
        <item quantity="one">I have %d cat</item>
        <item quantity="other">I have %d cats</item>
    </plurals>

</resources>